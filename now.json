{
  "version": 2,
  "name": "graphql-api",
  "scope": "telemark",
  "env": {
    "SERVICE_JWT": "@graphql-jwt",
    "SERVICE_JWT_EXPIRY_TIME": "2m",
    "DSF_SERVICE_URL": "@dsf2-service-url",
    "DSF_SERVICE_JWT": "@dsf-service-jwt",
    "KOR_SERVICE_URL": "@kor-service-url",
    "KOR_SERVICE_JWT": "@kor-service-jwt"
  },
  "builds": [
      { "src": "index.js", "use": "@now/node" },
      { "src": "*.md", "use": "@now/md" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "continue": true,
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Max-Age": "3600"
      }
    },
    {
      "src": "/(.*)",
      "status": 200,
      "methods": ["OPTIONS"],
      "dest": "/README.html"
    },
    { "src": "/graphql", "dest": "/index.js" },
    { "src": "/docs", "dest": "/README.html" },
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "/docs" }
    }
  ],
  "alias": "graphql.service.t-fk.no"
}